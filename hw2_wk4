matrix= [
  [5, 11, 3],
  [8, 1, 6],
  [98, 5, 11],
  [0, 9, 4]
  ]
  
c=0

p1=float(input('Please, enter first value to compare:'))
p2=float(input('Please, enter second value to compare:'))

for i in matrix:
  for j in i:
    c+=1 if p1<=j<=p2 else 0
print('The condition of {}<=matrix(i,j)<={} is true for {} values'.format(p1, p2, c))
